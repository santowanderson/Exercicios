Switch Core 11
 
enable
configure terminal 
hostname SW-CORE-11
ner motd "ACESSO APENAS PARA PESSOAS AUTORIZADAS!"
domain-name rede1.local
crypto key generate rsa general-keys modulus 1024
username wanderson privilege 15 secret wanderson15
line console 0
login local
exit
line vty 0 15
password SenhadaVTY
transport input ssh
exec-timeout 10
login local
exit
service password-encryption
vlan 10
name REDE1
vlan 20
name REDE2
vlan 100
name Rede de gerenciamento
exit
interface fastEthernet 0/1
description VLAN 10
switchport mode access
switchport access vlan 10
exit
interface fastEthernet 0/2
description VLAN 20
switchport mode access
switchport access vlan 20
exit
interface fastEthernet 0/24
description VLAN 100
switchport mode access
switchport access vlan 100
exit
interface gigabitEthernet 0/1
description INTERFACE TRUNK
switchport mode trunk
switchport trunk native vlan 99
switchport trunk allowed vlan 10,20,99,100
exit
interface vlan 99
ip address 172.16.1.254 255.255.255.240
description Rede de gerenciamento
no shutdown
exit
ip default-gateway 172.16.1.129
do wr




